
<div class="container px-5 px-sm-0">
  <div class="row">
    <div class="col-md-3">
      <%= render 'users/userinfo', user: @user, is_room: @is_room, room_id: @room_id, room: @room, entry: @entry, book: @book %>
    </div>
    <div class="col-md-8 offset-md-1">
      <h2>Books</h2>
      <div class="book_index">
        <%= render "books/index", books: @user.books %>
      </div>
    </div>
  </div>
</div>



